<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/container_qr_code_generated_organizer"
  android:layout_width="match_parent"
  android:layout_height="807dp"
  android:background="@drawable/container_qr_code_generated_organizer"
  android:gravity="left|top"
  android:orientation="vertical">

  <FrameLayout
      android:id="@+id/profile_container"
      android:layout_width="324dp"
      android:layout_height="wrap_content"
      android:layout_marginTop="23dp"
      android:layout_marginStart="25dp">

    <ImageView
        android:id="@+id/back_qr_share"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginEnd="2.79dp"
        android:scaleType="centerInside"
        android:src="@drawable/profile_vector_image" />

    <TextView
        android:id="@+id/profile_qr_text"
        android:layout_width="match_parent"
        android:layout_height="25dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginStart="36.5dp"
        android:layout_marginTop="-3.5dp"
        android:layout_marginBottom="-3.5dp"
        android:gravity="left|center_vertical"
        android:includeFontPadding="false"
        android:maxLines="1"
        android:singleLine="true"
        android:text="QR Code Generated "
        android:textColor="#FF000000"
        android:textSize="20.02dp" />
  </FrameLayout>
  
  <FrameLayout
    android:id="@+id/container_group"
    android:layout_width="345.92dp"
    android:layout_height="348.94dp"
    android:layout_marginTop="158.87dp"
      android:layout_marginStart="25dp">
    
    <ImageView
      android:id="@+id/qrImage"
      android:layout_width="247dp"
      android:layout_height="match_parent"
      android:layout_marginStart="50.94dp"
      android:layout_marginTop="38.27dp"
      android:layout_marginBottom="37.67dp"
      android:layout_gravity="left"
      android:scaleType="centerCrop" />
    
    <ImageView
      android:id="@+id/image1"
      android:layout_width="57.72dp"
      android:layout_height="54.76dp"
      android:layout_marginStart="288.2dp"
      android:layout_marginEnd="0dp"
      android:layout_marginBottom="294.18dp"
      android:scaleType="centerInside"
      android:src="@drawable/image1" />
    
    <ImageView
      android:id="@+id/image2"
      android:layout_width="57.72dp"
      android:layout_height="54.76dp"
      android:layout_marginStart="288.2dp"
      android:layout_marginEnd="0dp"
      android:layout_marginTop="294.18dp"
      android:scaleType="centerInside"
      android:src="@drawable/image2" />
    
    <ImageView
      android:id="@+id/image3"
      android:layout_width="57.72dp"
      android:layout_height="54.76dp"
      android:layout_marginStart="5.24dp"
      android:layout_marginEnd="282.96dp"
      android:layout_marginBottom="294.18dp"
      android:scaleType="centerInside"
      android:src="@drawable/image3" />
    
    <ImageView
      android:id="@+id/image4"
      android:layout_width="57.72dp"
      android:layout_height="54.76dp"
      android:layout_marginStart="0dp"
      android:layout_marginEnd="288.2dp"
      android:layout_marginTop="294.18dp"
      android:scaleType="centerInside"
      android:src="@drawable/image4" />
  </FrameLayout>
  
  <LinearLayout
    android:id="@+id/share_generated_qr"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginStart="116dp"
    android:layout_marginTop="48.33dp"
    android:layout_gravity="left"
    android:clipToPadding="false"
    android:gravity="left|center_vertical"
    android:orientation="horizontal">
    
    <ImageView
      android:id="@+id/image_icon_export"
      android:layout_width="41dp"
      android:layout_height="41dp"
      android:scaleType="centerInside"
      android:src="@drawable/share_not_filled" />
    
    <TextView
      android:id="@+id/text_share_now"
      android:layout_width="wrap_content"
      android:layout_height="21dp"
      android:layout_marginStart="14.29dp"
      android:gravity="center"
      android:includeFontPadding="false"
      android:maxLines="1"
      android:singleLine="true"
      android:text="Share Now"
      android:textColor="#FF000000"
      android:textSize="16.02dp" />
  </LinearLayout>
  
  <FrameLayout
    android:id="@+id/container_button"
    android:layout_width="222.88dp"
    android:layout_height="45.61dp"
    android:layout_marginStart="5.94dp"
    android:layout_marginTop="53dp"
    android:layout_gravity="center_horizontal"
    android:background="@drawable/container_button">
    

  </FrameLayout>
</LinearLayout>
