<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_event_information_page"
    android:layout_width="match_parent"
    android:layout_height="807dp"
    android:background="@drawable/container_event_information_page"
    tools:context=".MainActivity">


  <ImageView
      android:id="@+id/bottom_actionbar"
      android:layout_width="395dp"
      android:layout_height="74dp"
      android:layout_gravity="top|start"
      android:layout_marginStart="-2dp"
      android:layout_marginTop="682dp"
      android:background="@drawable/bottom_actionbar"
      android:scaleType="centerInside" />

  <ImageView
      android:id="@+id/attendees_admin_browse_event"
      android:layout_width="38dp"
      android:layout_height="38dp"
      android:layout_marginStart="52.38dp"
      android:layout_marginTop="700dp"
      android:layout_marginEnd="314.43dp"
      android:layout_marginBottom="30.81dp"
      android:scaleType="centerInside"
      android:src="@drawable/attendees_black" />

  <ImageView
      android:id="@+id/home_event_details_org"
      android:layout_width="38dp"
      android:layout_height="38dp"
      android:layout_marginStart="172dp"
      android:layout_marginTop="700dp"
      android:scaleType="centerInside"
      android:src="@drawable/home"/>

  <ImageView
      android:id="@+id/shareQR_event_details"
      android:layout_width="38dp"
      android:layout_height="38dp"
      android:layout_marginStart="300dp"
      android:layout_marginTop="700dp"
      android:layout_marginBottom="0.05dp"
      android:scaleType="centerInside"
      android:src="@drawable/share" />


  <FrameLayout
      android:id="@+id/container_group1"
      android:layout_width="259dp"
      android:layout_height="92dp"
      android:layout_marginLeft="61dp"
      android:layout_marginTop="400dp"
      android:layout_gravity="left"
      android:background="@drawable/container_group1">

    <ImageView
        android:id="@+id/image_rectangle1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/image_rectangle1"
        android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/text_attendee"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:padding="10dp"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Promotional QR code"
        android:textColor="#FFFFFFFF"
        android:textSize="30dp" />
  </FrameLayout>

  <FrameLayout
      android:id="@+id/container_group2"
      android:layout_width="259dp"
      android:layout_height="92dp"
      android:layout_marginLeft="61dp"
      android:layout_marginTop="210dp"
      android:layout_gravity="left"
      android:background="@drawable/container_group2">

    <ImageView
        android:id="@+id/image_rectangle2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/image_rectangle2"
        android:scaleType="centerCrop" />

    <TextView
        android:id="@+id/text_organizer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:padding="10dp"
        android:maxLines="1"
        android:singleLine="true"
        android:text="Check-In QR code"
        android:textColor="#FFFFFFFF"
        android:textSize="30dp" />
  </FrameLayout>


</FrameLayout>
